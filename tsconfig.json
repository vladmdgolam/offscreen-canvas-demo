{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "target": "es6",
    "module": "commonjs",
    "outDir": "build",
    "strict": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
